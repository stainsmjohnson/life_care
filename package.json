    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "cd android && ./gradlew clean",
    "android": "ENVFILE=./env/.env.development react-native run-android --variant=developmentdebug",
    "android:release": "ENVFILE=./env/.env.development react-native run-android --variant=developmentrelease",
    "android:stage": "ENVFILE=./env/.env.staging react-native run-android --variant=stagingdebug",
    "android:stage-release": "ENVFILE=./env/.env.staging react-native run-android --variant=stagingrelease",
    "android:prod": "ENVFILE=./env/.env.production react-native run-android --variant=productiondebug",
    "android:prod-release": "ENVFILE=./env/.env.production react-native run-android --variant=productionrelease",
    "cp:android": "ENVFILE=./env/.env.development appcenter codepush release-react -a stains.mj/TemplateRN-Android -d Development",
    "cp:android:stage": "ENVFILE=./env/.env.staging appcenter codepush release-react -a stains.mj/TemplateRN-Android -d Staging",
    "cp:android:prod": "ENVFILE=./env/.env.production appcenter codepush release-react -a stains.mj/TemplateRN-Android -d Production"
  },
